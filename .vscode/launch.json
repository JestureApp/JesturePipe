    "version": "0.2.0",
    "configurations": [
        {
            "name": "JesturePipe Demo (gdb)",
            "preLaunchTask": "Build JesturePipe Demo (debug)",
            "type": "cppdbg",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceRoot}/bazel-bin/jesturepipe/examples/jesturepipe_demo",
            "environment": [
                {
                    "name": "GLOG_logtostderr",
                    "value": "1"
                },
                {
                    "name": "GLOG_v",
                    "value": "1"
                }
            ],
            "args": []
        }
    ]
}